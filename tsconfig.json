{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "noImplicitReturns": true,
        // "noUnusedLocals": true,
        "outDir": "dist",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "target": "ES5",
        "skipLibCheck": true
    },
    "compileOnSave": false,
    "include": ["app"]
}
